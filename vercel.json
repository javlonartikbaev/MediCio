{
    "builds": [{
        "src": "MediCio/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.2" }
    },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "static/**", "use": "@vercel/static" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "MediCio/wsgi.py"
        }
    ]
}